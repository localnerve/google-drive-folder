{
  "root": true,
  "env": {
    "node": true,
    "es2020": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [2, 2, {
      "SwitchCase": 1,
      "MemberExpression": 1
    }],
    "quotes": [2, "single"],
    "dot-notation": [2, {"allowKeywords": true}]
  }
}