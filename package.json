{
  "name": "@localnerve/google-drive-folder",
  "version": "4.1.0",
  "description": "Stream files from a Google Drive folder",
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "node -e 'try{require(\"fs\").symlinkSync(\"../lib\", \"./__tests__/lib\");}catch(e){}'",
    "test": "jest",
    "transpile": "rimraf ./dist && babel --out-dir ./dist index.js && babel --out-dir ./dist/lib ./lib",
    "prepublishBuild": "node ./prepublishBuild.js",
    "prepublishOnly": "npm run transpile && npm run prepublishBuild",
    "test:private-outdir": "node private/v0.4.0/private-outdir.js",
    "test:private-stream": "node private/v0.4.0/private-stream.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localnerve/google-drive-folder.git"
  },
  "author": "Alex Grant <alex@localnerve.com> (https://www.localnerve.com)",
  "maintainers": [
    {
      "name": "localnerve",
      "email": "alex@localnerve.com",
      "url": "https://github.com/localnerve"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/localnerve/google-drive-folder/issues"
  },
  "homepage": "https://github.com/localnerve/google-drive-folder#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1"
  },
  "dependencies": {
    "@googleapis/drive": "^5.1.0"
  },
  "engines": {
    "node": ">=16"
  }
}
